    <RootNamespace>PDFtoPrinter.Sample</RootNamespace>
    <AssemblyName>PDFtoPrinter.Sample</AssemblyName>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\PDFtoPrinter\PDFtoPrinter.csproj" />
